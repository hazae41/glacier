    "rimraf": "^3.0.2",
    "ttypescript": "^1.5.13",
    "typescript": "^4.8.4",
    "typescript-transform-paths": "^3.3.1"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js"
    }
