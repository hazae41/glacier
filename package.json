{
  "type": "module",
  "name": "@hazae41/xswr",
  "version": "1.0.91",
  "author": "hazae41",
  "license": "MIT",
  "description": "The simplest React data (re)fetching library ever made",
  "homepage": "https://github.com/hazae41/xswr",
  "types": "./dist/types/index.d.ts",
  "module": "./dist/mjs/src/index.mjs",
  "main": "./dist/cjs/src/index.cjs",
